{
    "name": "Wayback machine",
    "description": "View current page on wayback machine: archive.is",
    "version": "0.1",
    "author": "Yankee Maharjan",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": ["activeTab"],
    "browser_action": {
        "default_icon": {
            "16": "assets/16x16.png",
            "48": "assets/48x48.png",
            "128": "assets/128x128.png"
        }
    }
}
